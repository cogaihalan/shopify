  /* Interactive
    * ========================================================================== */

  /*
    * Add the correct styles in Safari.
    */

  dialog {
    position: absolute;
    right: 0;
    left: 0;

    width: -moz-fit-content;
    width: -webkit-fit-content;
    width: fit-content;
    height: -moz-fit-content;
    height: -webkit-fit-content;
    height: fit-content;
    margin: auto;
    padding: 1em;

