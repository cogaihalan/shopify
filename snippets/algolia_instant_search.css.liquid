  }
}
.src-Toolbar_Trigger-filters .src-Toolbar_TriggerCount::before {
  content: '(';

  display: inline-block;
}
.src-Toolbar_Trigger-filters .src-Toolbar_TriggerCount::after {
  content: ')';

  display: inline-block;
}

